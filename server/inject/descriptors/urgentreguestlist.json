{
  "type": "object",
  "properties": {
    "urgentreguestlist": {
      "type": "array",
      "selectorAll": "#requestlist li",
      "items": {
        "type": "object",
        "properties": {
          "company": {
            "type": "string",
            "selector": "#requestlist > li > div > div.col-sm-1 > div > span:nth-child(1)",
            "control": "property.innerText"
          }
        }
      }
    },
    "isurgentlist": {
      "type": "identifier",
      "selector": "#requestlist",
      "control": "exists"
    }
  },
  "connect": [
    {
      "match": [
        "isurgentlist"
      ],
      "screens": [
        "urgentreguestlist"
      ]
    }
  ]
}
